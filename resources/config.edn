{:application/environment :dev

 :http/server {:config {:port 7700} :handler #ig/ref :http/handler}

 :http/handler {:dependencies {:conn #ig/ref :datomic/conn}}

 :servitor/config {:conn #ig/ref :datomic/conn
                   :services [{:task :vasco.servitor.task/init-todos
